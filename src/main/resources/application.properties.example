spring.jpa.hibernate.ddl-auto=create-drop

spring.jackson.default-property-inclusion=NON_NULL

spring.datasource.driver-class-name=com.mysql.cj.jdbc.Driver
management.endpoints.enabled-by-default=true
management.endpoints.web.exposure.include=*

spring.mvc.view.prefix=/views/
spring.mvc.view.suffix=.jsp
spring.servlet.multipart.max-file-size=10MB
spring.servlet.multipart.max-request-size=10MB

spring.mvc.hiddenmethod.filter.enabled=true

spring.datasource.url=jdbc:mysql://mygarden-mysql:3306/amt-mygarden
spring.datasource.username=mygarden
spring.datasource.password=password

logging.level.org.springframework=TRACE
logging.level.com=TRACE

spring.jpa.show-sql=true

logging.level.org.springframework.web=DEBUG
logging.level.org.hibernate=ERROR

login-api.path=http://login-microservice

s3.key.id=TODO
s3.key.secret=TODO
s3.region.name=eu-north-1
s3.bucket.name=mygarden.diduno.education


